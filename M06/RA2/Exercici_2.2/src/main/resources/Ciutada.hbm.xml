<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.project.Ciutada" table="CIUTADA">
        <id name="ciutadaId" column="CIUTADA_ID">
            <generator class="native"/>
        </id>
        <property name="nom" column="NOM" type="string" not-null="true"/>
        <property name="cognom" column="COGNOM" type="string" not-null="true"/>
        <property name="edat" column="EDAT" type="int"/>
    </class>
</hibernate-mapping>